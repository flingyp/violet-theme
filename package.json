{
  "name": "violet-theme",
  "displayName": "Violet Theme",
  "version": "0.0.0",
  "packageManager": "pnpm@7.12.1",
  "description": "Violet Theme for VS Code",
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "vitesse",
    "light",
    "dark"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/flingyp/vscode-theme-violet.git"
  },
  "files": [
    "themes"
  ],
  "bugs": {
    "url": "https://github.com/flingyp/vscode-theme-violet/issues"
  },
  "publisher": "FlyPeng",
  "scripts": {
    "build": "esno src/index.ts",
    "dev": "nodemon --watch src -e ts --exec pnpm build",
    "lint": "eslint ."
  },
  "contributes": {
    "themes": [
      {
        "label": "Violet Light",
        "uiTheme": "vs",
        "path": "./themes/violet-light.json"
      },
      {
        "label": "Violet Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/violet-dark.json"
      }
    ]
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.19.4",
    "@antfu/utils": "^0.5.0",
    "@types/color": "^3.0.3",
    "@types/node": "^17.0.23",
    "color": "^4.2.3",
    "eslint": "^8.12.0",
    "esno": "^0.14.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.7.1",
    "tsup": "^5.12.4",
    "typescript": "^4.6.3",
    "vsce": "^2.7.0"
  },
  "engines": {
    "vscode": "^1.43.0"
  },
  "icon": "icon.png",
  "preview": true
}
